{
  "name": "dev-board",
  "version": "1.0.0",
  "description": "A comprehensive productivity dashboard with real-time features, AI insights, and advanced analytics",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr:DevBoard": "node dist/dev-board/server/server.mjs",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/dev-board/stats.json"
  },
  "keywords": [
    "productivity",
    "dashboard",
    "angular",
    "real-time",
    "analytics",
    "ai-insights"
  ],
  "private": true,
  "dependencies": {
